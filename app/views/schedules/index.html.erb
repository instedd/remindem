<h1>Your schedules</h1>

<table>
  <tr>
    <th>Keyword</th>
    <th>Time Scale</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @schedules.each do |schedule| %>
  <tr>
    <td><%= schedule.keyword %></td>
    <td><%= schedule.timescale %></td>
    <td><%= link_to 'Show', schedule_path(schedule) %></td>
    <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
    <td><%= link_to 'Destroy', schedule_path(schedule), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Schedule', new_schedule_path %>
